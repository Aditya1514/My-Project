@model List<WebApplication10.Models.Brand>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutPage1.cshtml";
}

<h2>Index</h2>
<form class=" form-inline" action="/Brands/index">
    <label for="search">Search</label>
    <input type="text" class="form-control" id="search" name="search" placeholder="search" value="@ViewBag.search" /><br />
    <button class="btn btn-primary">GO</button>
</form>
<table class=" table">
    <tr>
        <th>BrandID</th>
        <th>BrandName</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.BrandID</td>
            <td>@item.BrandName</td>
            <td>
                <a style="background-color :black" , href="/Brands/details/@item.BrandID">Details</a>
               <a href="/Brands/edit/@item.BrandID">Edit</a>
            </td>
        </tr>
    }
</table>