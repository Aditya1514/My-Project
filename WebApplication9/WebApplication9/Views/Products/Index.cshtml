
@Model List<WebApplication9.Models.Products>
    @{
        ViewBag.Title = "Products";
        Layout = "~/Views/Shared/_LayoutPage1.cshtml";
    }

    <h2>Products</h2>

    <table class=" table">
        <tr>
            <th>ProductID</th>
            <th>ProductName</th>
            <th>Price</th>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.ProductID</td>
                <td>@item.ProductName</td>
                <td>@item.Price</td>
            </tr>
        }
    </table>

