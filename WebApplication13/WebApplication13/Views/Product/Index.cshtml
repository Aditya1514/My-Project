@model List<WebApplication13.Models.Product>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutPage1.cshtml";
}

<h2>Products</h2>
<a href="Create/Product">Create Product</a>
<table class="table">
    <tr>
        <th>ProductID</th>
        <th>ProductName</th>
        <th>Price</th>
        <th>DateOfPurchase</th>
        <th>CategoryID</th>
        <th>BrandID</th>
    </tr>
    @foreach(var item in Model)
            {
<tr>
    <td>@item.ProductID</td>
    <td>@item.ProductName</td>
    <td>@item.Price</td>
    <td>@item.DateOfPurchase</td>
    <td>@item.CategoryID</td>
    <td>@item.BrandID</td>
</tr>
            }
</table>

